{
  "name": "xm-webapp",
  "version": "2.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/xm-online/xm-webapp.git"
  },
  "scripts": {
    "ng": "ng",
    "install-exts": "node ./scripts/install-exts.js",
    "start": "npm run prebuild && ng serve --proxy-config proxy.conf.js --host=0.0.0.0",
    "start-local": "npm run prebuild && ng serve --proxy-config local.proxy.conf.js --live-reload",
    "start-local:aot": " npm run prebuild && node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng serve --proxy-config local.proxy.conf.js --live-reload  --host=0.0.0.0 --prod --aot=true",
    "dev": "npm run prebuild && ng serve --proxy-config local.proxy.conf.js --live-reload --host=0.0.0.0",
    "dev:aot": "npm run prebuild && ng serve --proxy-config local.proxy.conf.js --live-reload  --host=0.0.0.0 --prod --aot=true",
    "build": "npm run prebuild && ng build --stats-json",
    "build-aot": "npm run prebuild && node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng build --prod --aot=true",
    "build-no-aot": "npm run prebuild && ng build --prod --aot=false",
    "build-prod-v1": "npm run prebuild && ng build -pr --prod",
    "build-prod-v1-stat": "npm run prebuild && ng build -pr --prod --statsJson",
    "build-prod-v2": "npm run prebuild && node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng build --prod --aot=true --build-optimizer=true --vendor-chunk=true --common-chunk=true",
    "build-prod-v2-stat": "npm run prebuild && node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng build --prod --aot=true --build-optimizer=true --vendor-chunk=true --common-chunk=true --statsJson",
    "prebuild": "npm run mng-ext && npm run install-exts && node ./scripts/merge-translations.js",
    "mng-ext": "node ./scripts/manage-exts.js",
    "test": "npm run prebuild && ng test --source-map=false",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "get-unwraped-labels": "node ./scripts/get-unwraped-labels.js",
    "translations": "node ./scripts/translations.js",
    "report:bundle": "webpack-bundle-analyzer dist/stats.json"
  },
  "private": true,
